{
  "name": "@sdkset/core",
  "version": "4.0.11",
  "description": "Axios re encapsulation",
  "author": "True",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "publish:package": "npm publish",
    "rimraf:package": "rimraf dist/* -g",
    "vitest:package": "vitest run -c vitest.config.unit.ts",
    "build:package": "pnpm rimraf:package &&  rollup --bundleConfigAsCjs -c ../../rollup.config.js  --environment target:core"
  },
  "keywords": [
    "@sdkset/utils",
    "storage",
    "format",
    "dayjs",
    "axios",
    "WebSocket"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "import": "./dist/index.js",
    "default": "./dist/index.cjs.js",
    "types": "./dist/index.d.ts"
  },
  "module": "dist",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "browser": "dist/index.umd.js",
  "buildOptions": {
    "name": "sdkCore",
    "formats": [
      "es",
      "cjs",
      "umd"
    ],
    "globals": {
      "@sdkset/mode": "sdkMode",
      "dayjs": "dayjs",
      "axios": "axios"
    }
  },
  "devDependencies": {
    "dayjs": "^1.11.7",
    "axios": "^1.4.0",
    "@sdkset/mode": "workspace:*",
    "@sdkset/types": "workspace:*"
  },
  "dependencies": {},
  "peerDependencies": {
    "dayjs": "^1.11.7",
    "axios": "^1.4.0",
    "@sdkset/mode": "*",
    "@sdkset/types": "*"
  }
}