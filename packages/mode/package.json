{
  "name": "@sdkset/mode",
  "version": "1.0.0",
  "description": "Mode re encapsulation",
  "author": "True",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "publish:version": "npm version %npm_config_version%",
    "publish:package": "npm publish",
    "rimraf:package": "rimraf dist/* -g",
    "test:package": "vitest run -c vitest.config.unit.ts",
    "build:package": "pnpm rimraf:package &&  rollup --bundleConfigAsCjs -c ../../rollup.config.js  --environment target:mode"
  },
  "keywords": [
    "@sdkset/utils",
    "model"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    },
    "./babel": {
      "import": "./dist/babel.es.js",
      "require": "./dist/babel.cjs.js"
    }
  },
  "main": "dist/index.cjs.js",
  "babel_main": "dist/babel.cjs.js",
  "module": "dist",
  "babel_module": "dist/babel.es.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "buildOptions": {
    "name": "mode",
    "formats": [
      "es",
      "babel_es",
      "cjs",
      "babel_cjs",
      "umd"
    ]
  },
  "dependencies": {
    "@sdkset/types": "workspace:*"
  },
  "peerDependencies": {
    "@sdkset/types": "*"
  },
  "standard-version": {
    "skip": {
      "bump": false,
      "changelog": true,
      "commit": true,
      "tag": true
    }
  }
}
