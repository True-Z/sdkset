{
  "name": "@sdkset/numeral",
  "version": "1.0.5",
  "description": "Numeral re encapsulation",
  "author": "True",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "version:package": "standard-version",
    "publish:package": "npm publish",
    "rimraf:package": "rimraf dist/* -g",
    "test:package": "vitest run -c vitest.config.unit.ts",
    "build:package": "pnpm rimraf:package &&  rollup --bundleConfigAsCjs -c ../../rollup.config.js  --environment target:numeral"
  },
  "keywords": [
    "@sdkset/utils",
    "numeral"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    },
    "./babel": {
      "import": "./dist/babel.es.js",
      "require": "./dist/babel.cjs.js"
    }
  },
  "main": "dist/index.cjs.js",
  "babel_main": "dist/babel.cjs.js",
  "module": "dist",
  "babel_module": "dist/babel.es.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "buildOptions": {
    "name": "*",
    "formats": [
      "es",
      "babel_es",
      "cjs",
      "babel_cjs"
    ]
  },
  "dependencies": {
    "@sdkset/types": "workspace:*",
    "@types/numeral": "^2.0.2",
    "numeral": "^2.0.6"
  },
  "peerDependencies": {
    "@sdkset/types": "*",
    "@types/numeral": "^2.0.2",
    "numeral": "^2.0.6"
  },
  "standard-version": {
    "skip": {
      "bump": false,
      "changelog": true,
      "commit": true,
      "tag": true
    }
  }
}
