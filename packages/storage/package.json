{
  "name": "@sdkset/storage",
  "version": "3.1.2",
  "description": "Storage re encapsulation",
  "author": "True",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "publish:package": "npm publish",
    "rimraf:package": "rimraf dist/* -g",
    "vitest:package": "vitest run -c vitest.config.unit.ts",
    "build:package": "pnpm rimraf:package &&  rollup --bundleConfigAsCjs -c ../../rollup.config.js  --environment target:storage"
  },
  "keywords": [
    "@sdkset/utils",
    "storage"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "import": "./dist/index.js",
    "default": "./dist/index.cjs.js",
    "types": "./dist/index.d.ts"
  },
  "module": "dist",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "browser": "dist/index.umd.js",
  "buildOptions": {
    "name": "sdkStorage",
    "formats": [
      "es",
      "cjs",
      "umd"
    ]
  },
  "devDependencies": {
    "@sdkset/types": "workspace:*"
  },
  "dependencies": {},
  "peerDependencies": {
    "@sdkset/types": "*"
  }
}
