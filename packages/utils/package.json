{
  "name": "@sdkset/utils",
  "version": "1.0.0",
  "description": "Re encapsulation of commonly used libraries",
  "author": "True",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "publish:version": "npm version %npm_config_version%",
    "publish:package": "npm publish",
    "rimraf:package": "rimraf dist/* -g",
    "test:package": "vitest run -c vitest.config.unit.ts",
    "build:package": "pnpm rimraf:package &&  rollup --bundleConfigAsCjs -c ../../rollup.config.js  --environment target:utils"
  },
  "keywords": [
    "@sdkset/utils",
    "axios",
    "underscore",
    "dayjs",
    "numeral",
    "socket",
    "storage",
    "types"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    },
    "./babel": {
      "import": "./dist/babel.es.js",
      "require": "./dist/babel.cjs.js"
    }
  },
  "main": "dist/index.cjs.js",
  "babel_main": "dist/babel.cjs.js",
  "module": "dist",
  "babel_module": "dist/babel.es.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "buildOptions": {
    "name": "*",
    "formats": [
      "cjs",
      "es"
    ]
  },
  "dependencies": {
    "@sdkset/axios": "workspace:*",
    "@sdkset/core": "workspace:*",
    "@sdkset/dayjs": "workspace:*",
    "@sdkset/mode": "workspace:*",
    "@sdkset/numeral": "workspace:*",
    "@sdkset/socket": "workspace:*",
    "@sdkset/storage": "workspace:*",
    "@sdkset/types": "workspace:*"
  },
  "peerDependencies": {
    "@sdkset/axios": "*",
    "@sdkset/core": "*",
    "@sdkset/dayjs": "*",
    "@sdkset/mode": "*",
    "@sdkset/numeral": "*",
    "@sdkset/socket": "*",
    "@sdkset/storage": "*",
    "@sdkset/types": "*"
  }
}
