{
  "name": "@sdkset/tools",
  "version": "3.3.2",
  "description": "Function library implementation",
  "author": "True",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "publish:package": "npm publish",
    "rimraf:package": "rimraf dist/* -g",
    "vitest:package": "vitest run -c vitest.config.unit.ts",
    "build:package": "pnpm rimraf:package &&  rollup --bundleConfigAsCjs -c ../../rollup.config.js  --environment target:tools"
  },
  "keywords": [
    "@sdkset/utils",
    "underscore",
    "tools"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "import": "./dist/index.js",
    "default": "./dist/index.cjs.js",
    "types": "./dist/index.d.ts"
  },
  "module": "dist",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "browser": "dist/index.umd.js",
  "buildOptions": {
    "name": "sdkset",
    "formats": [
      "es",
      "cjs",
      "umd"
    ],
    "globals": {
      "@sdkset/mode": "sdkset"
    }
  },
  "devDependencies": {
    "@sdkset/types": "workspace:*",
    "@sdkset/mode": "workspace:*"
  },
  "dependencies": {},
  "peerDependencies": {
    "@sdkset/types": "*",
    "@sdkset/mode": "*"
  }
}
